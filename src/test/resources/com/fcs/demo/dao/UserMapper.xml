<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fcs.demo.dao.UserMapper" >

    <select id="getUserById" resultType="com.fcs.model.User">
       select * from tb_user where id = #{id}
    </select>

    <select id="selectUserList" resultType="com.fcs.model.User">
        select * from tb_user
    </select>

</mapper>